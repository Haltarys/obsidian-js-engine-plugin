{
	"name": "obsidian-js-engine-plugin",
	"version": "0.0.5",
	"description": "This plugin for obsidian allows you to use and run JavaScript from within your notes.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.dev.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest",
		"format": "prettier --write .",
		"lint": "eslint --max-warnings=0 src/**",
		"types": "tsc --declaration --emitDeclarationOnly --outfile index.d.ts",
		"docs": "node docs.js"
	},
	"keywords": [],
	"author": "Moritz Jung",
	"license": "GPL-3.0",
	"devDependencies": {
		"@codemirror/lang-javascript": "^6.1.9",
		"@codemirror/view": "^6.14.0",
		"@tsconfig/svelte": "^5.0.0",
		"@types/jest": "^29.5.2",
		"@types/node": "^20.3.2",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.17.0",
		"esbuild-plugin-copy-watch": "^2.0.0",
		"esbuild-svelte": "^0.7.1",
		"eslint-plugin-only-warn": "^1.0.3",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"obsidian": "latest",
		"obsidian-dataview": "latest",
		"prettier": "2.8.8",
		"svelte-preprocess": "^5.0.4",
		"ts-jest": "^29.1.0",
		"tslib": "2.6.0",
		"typedoc": "^0.24.8",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"@lemons_dev/parsinom": "^0.0.11",
		"@popperjs/core": "^2.11.8",
		"itertools-ts": "^1.27.0",
		"obsidian-svelte": "^0.1.9",
		"svelte": "^4.0.1",
		"svelte-portal": "^2.2.0"
	}
}
